{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Just the basicsâ„¢ ",
  "main": "server.js",
  "repository": "git@github.com:verkstedt/react-boilerplate.git",
  "author": "Georgios Giannoutsos Barkas <georgios@verkstedt.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.34",
    "@babel/preset-env": "^7.0.0-beta.32",
    "@babel/preset-react": "^7.0.0-beta.32",
    "@verkstedt/eslint-config-verkstedt": "^5.0.1",
    "babel-core": "^7.0.0-0",
    "babel-loader": "8.0.0-beta.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-jest": "^21.2.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.3.0-beta.11",
    "react-test-renderer": "^16.2.0",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.5"
  },
  "dependencies": {
    "babel-jest": "^21.2.0",
    "babel-polyfill": "^6.26.0",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-crud": "^3.2.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src"
    ],
    "moduleFileExtensions": [
      "js",
      "css"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/__tests__/setup.js"
    ],
    "testMatch": [
      "**/__tests__/**/*test.js?(x)"
    ]
  }
}
