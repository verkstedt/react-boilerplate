{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Just the basicsâ„¢ ",
  "main": "server.js",
  "repository": "git@github.com:verkstedt/react-boilerplate.git",
  "author": "Georgios Giannoutsos Barkas <georgios@verkstedt.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --mode development --open",
    "build": "webpack --config webpack.config.prod.js --mode production",
    "test": "jest",
    "analyze": "source-map-explorer dist/bundle.js"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.42",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-react": "^7.0.0-beta.42",
    "@babel/register": "^7.0.0-beta.42",
    "@verkstedt/eslint-config-verkstedt": "^5.0.2",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^8.0.0-beta",
    "babel-preset-jest": "^22.4.3",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "optimize-css-assets-webpack-plugin": "^4.0.1",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^7.0.1",
    "source-map-explorer": "^1.5.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-hot-loader": "^4.1.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-crud": "^3.2.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src"
    ],
    "moduleFileExtensions": [
      "js",
      "scss"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__tests__/fileMock.js",
      "\\.(scss|less)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/__tests__/setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testMatch": [
      "**/__tests__/**/*test.js?(x)"
    ]
  }
}
