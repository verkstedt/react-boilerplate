{
  "presets": [
    ["@babel/preset-env", {
      "modules": false,
      "useBuiltIns": "usage"
    }],
    "@babel/preset-react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
  ],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"],
      "presets": [
        ["@babel/preset-env", {
          "modules": false,
        }],
        "@babel/preset-react"
      ],
    }
  }
}
